
CPPFLAGS:=-I../../include @CPPFLAGS@
LDFLAGS:=-L../../ @LDFLAGS@ 

all: test module

test: test.cc
	$(CXX) $(DEFINES) $(CPPFLAGS) $(LDFLAGS) $(CFLAGS) $(DEBUG) -ldodo $^ -o $@ 

module: module.cc
	$(CXX) $(DEFINES) $(CPPFLAGS) $(LDFLAGS) $(CFLAGS) $(DEBUG) -Wall -shared -ldodo -fPIC $^ -o $@ 
	
clean:
	rm -rf test module
